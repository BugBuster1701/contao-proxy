{
	"name":"bugbuster/contao-proxy",
	"description":"Contao module. Proxy support for the extension catalog and the extension manager",
	"keywords":["contao","proxy"],
	"type":"contao-module",
	"homepage":"http://contao.ninja",
	"license":"LGPL-3.0+",
	"authors":[
		{
			"name":"Glen Langer",
			"homepage":"http://contao.ninja",
			"role":"Developer"
		}
	],
	"support":{
		"issues":"https://github.com/BugBuster1701/contao-proxy/issues",
		"irc":"irc://irc.freenode.org/contao.dev",
		"source":"https://github.com/BugBuster1701/contao-proxy",
		"forum":"https://community.contao.org/de/forumdisplay.php?119-Sonstige-Erweiterungen"
	},
	"require":{
		"php":">=5.4",
		"contao/core":">=3.2,<4",
		"contao-community-alliance/composer-plugin": "~2.4"
	},
	"autoload":{
		"classmap":[""]
	},
	"replace":{
		"contao-legacy/proxy":"*"
	},
	"extra":{
		"contao":{
			"sources":{
				"":"system/modules/proxy"
			},
			"transifex": {
				"project": "contao-proxy",
				"prefix": "master-",
				"languages_cto": "languages",
				"languages_tx": ".tx"
			}
		}
	}
}
